{
  "expo": {
    "name": "divvly",
    "slug": "divvly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "notification": {
      "icon": "./assets/images/icon.png"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-apple-authentication",
      "@logrocket/react-native",
       [
        "expo-image-picker",
        {
          "photosPermission": "Divvly uses yor photos for profile verification."
        }
       ]
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCs3fxia_UXztnwT9WJ-gnOGB30MGXF5_U"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCs3fxia_UXztnwT9WJ-gnOGB30MGXF5_U"
        }
      },
      "package": "com.niebex.divvly"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "66e442df-c84d-468f-8c52-f40c8e3902b5"
      }
    },
    "owner": "niebex"
  }
}
