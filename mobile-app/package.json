    "babel-loader": "8.3.0",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": ">=4.4.1 <5.0.0",
    "eslint-config-expo": "^6.0.0",
    "jest": "^26.6.3",
    "jest-expo": "~47.0.1",
    "json-server": "^0.17.3",
    "jsonwebtoken": "^9.0.0",
    "ngrok": "^4.3.3",
    "prettier": ">=1.5.3 <2.0.0",
    "react-native-dotenv": "^3.4.2",
    "react-native-storybook-loader": "^2.0.5",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "stripe": "^12.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "webpack-dev-server": "^3"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./components",
        "./screens"
      ],
      "pattern": "**/*.stories.{tsx,jsx,js,ts}",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "private": true
}
