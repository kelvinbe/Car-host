{
  "name": "divvly",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "serve": " cd ../app & npm run dev",
    "dev": "concurrently \" cd ../app & npm run dev \" \"npm run start \" \"npm run mock\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write !.expo/ ./**/*.{js,jsx,ts,tsx,css,md,json}  --config ./.prettierrc",
    "storybook": "start-storybook dev -p 7007",
    "prestorybook": "rnstl",
    "generate-db": "ts-node ./mock/generate.ts",
    "mock": "ts-node ./mock/server.ts"
  },
  "jest": {
    "preset": "@testing-library/react-native",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.test.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mock__/empty.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.1.6",
    "@emotion/core": "^10.0.27",
    "@expo-google-fonts/lato": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^0.17.2",
    "@gorhom/bottom-sheet": "^4.4.5",
    "@logrocket/react-native": "^1.8.1",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/slider": "4.2.4",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@stripe/stripe-react-native": "0.19.0",
    "axios": "^1.2.0",
    "dayjs": "^1.11.6",
    "expo": "~47.0.6",
    "expo-apple-authentication": "~5.0.1",
    "expo-asset": "~8.7.0",
    "expo-auth-session": "~3.8.0",
    "expo-background-fetch": "~11.0.1",
    "expo-constants": "~14.0.2",
    "expo-device": "~5.0.0",
    "expo-font": "~11.0.1",
    "expo-image-picker": "~14.0.2",
    "expo-linear-gradient": "~12.0.1",
    "expo-linking": "~3.3.1",
    "expo-location": "~15.0.1",
    "expo-modules-autolinking": ">=0.8.1",
